{
    "manifest_version": 2,
    "name": "url2qrcode",
    "version": "1.0",
    "description": "chrome插件用于将地址转为二维码",
	"permissions": ["tabs"],
    "icons": {
        "16": "images/logo.png",
        "48": "images/logo.png",
        "128": "images/logo.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/logo.png",
            "38": "images/logo.png"
        },
        "default_title": "地址转为二维码",
        "default_popup": "popup.html"
    }
}
